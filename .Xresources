Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.dpi: 93
Xft.rgba: rgb
Xft.lcdfilter: lcddefault




URxvt.scrollBar: false
URxvt.letterSpace: -1
URxvt*font:                 xft:DejaVu Sans Mono for Powerline:regular:size=12
URxvt*imFont:               xft:DejaVu Sans Mono for Powerline:regular:size=12 
URxvt*boldFont:             xft:DejaVu Sans Mono for Powerline:bold:size=12
URxvt*italicFont:           xft:DejaVu Sans Mono for Powerline:italic:size=12
URxvt*boldItalicFont:       xft:DejaVu Sans Mono for Powerline:bold:italic:size=12

URxvt.perl-ext-common: clipboard,resize-font

URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste


!!URxvt.transparent: true
!!URxvt.shading: 14
!!URxvt.foreground: #657b83
URxvt.depth: 32
URxvt.background: rgba:0000/2b00/3600/c800


!!Source http://github.com/altercation/solarized

